<p style="color: green"><%= notice %></p>
<br><br><br><br>
<h1>Admin Users</h1>

<div id="adminusers">
  <% @adminusers.each do |adminuser| %>
    <%= render adminuser %>
    <p>
      <%= link_to "Show this admin user", adminuser %>
    </p>
  <% end %>
</div>

<%= button_to "New admin user", new_adminuser_path, method: :get, class: "new-and-back-btn"%>
<%= link_to 'Help', '/admin_help', class: "new-and-back-btn" %>