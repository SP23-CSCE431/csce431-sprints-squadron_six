<div id="<%= dom_id alumni %>">
  <p>
    <strong>Id:</strong>
    <%= alumni.ID %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= alumni.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= alumni.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= alumni.email %>
  </p>

  <p>
    <strong>Graduation year:</strong>
    <%= alumni.graduation_year %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= alumni.company %>
  </p>

</div>
